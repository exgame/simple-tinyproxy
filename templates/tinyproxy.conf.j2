User tinyproxy
Group tinyproxy
Port {{ proxy_port }}
Timeout 600
Logfile "/var/log/tinyproxy/tinyproxy.log"
LogLevel Info
PidFile "/run/tinyproxy/tinyproxy.pid"
MaxClients 100
MinSpareServers 5
MaxSpareServers 20
StartServers 10
MaxRequestsPerChild 0
ViaProxyName "tinyproxy"
ConnectPort 443
connectPort 80
BasicAuth {{ proxy_username }} {{ proxy_password }}
{% for item in proxy_allow_list %}
Allow {{ item }}
{% endfor %}
